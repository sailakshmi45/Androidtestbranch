<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/menusliderlayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginBottom="60dp"
    android:layout_marginTop="60dp"
    android:background="@color/white" >

    <ScrollView
        android:layout_width="350dp"
        android:layout_height="fill_parent"
        android:background="@color/white"
        android:scrollbars="none"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/white"
            android:orientation="vertical" >

            <FrameLayout
                android:id="@+id/profilelayout"
                android:layout_width="fill_parent"
                android:layout_height="180dp"
                android:layout_gravity="center_vertical"
                android:background="@color/scanattendee_icons_color"
                android:gravity="center|center_horizontal"
                >


                <com.globalnest.classes.RoundedImageView
                    android:id="@+id/imgprofile"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="20dp"
                    android:padding="2dp"
                    android:background="@drawable/circle_shape"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/profilename"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="120dp"
                    android:singleLine="true"
                    android:text="Welcome"
                    android:textColor="@color/white"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <TextView
                    android:id="@+id/profileemail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="140dp"
                    android:singleLine="true"
                    android:text="Welcome"
                    android:textColor="@color/white"
                    android:textSize="12dp"
                    android:textStyle="bold|italic"
                    android:typeface="serif" />
            </FrameLayout>
            <FrameLayout
                android:id="@+id/horinzontal_line_home"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="57dp"
                android:background="@color/white" />
            <RelativeLayout
                android:id="@+id/eventlayout"
                android:layout_width="fill_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:background="@color/gray_selector_color"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imgevents"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/events" />

                <TextView
                    android:id="@+id/txtevents"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imgevents"
                    android:gravity="center_vertical"
                    android:text="@string/events"
                    android:textColor="@color/setting_bg"
                    android:textSize="@dimen/font_normal"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/eventline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:background="@color/scanattendee_dark_color"
                    android:visibility="gone" />
            </RelativeLayout>

            <FrameLayout
                android:id="@+id/horinzontal_line_events"
                android:layout_width="fill_parent"
                android:layout_height="3dp"
                android:background="@color/darl_gray_text_color" />
            <!-- <FrameLayout
                 android:id="@+id/horinzontal_line_home"
                 android:layout_width="fill_parent"
                 android:layout_height="1dp"
                 android:layout_marginLeft="57dp"
                 android:background="@color/header_stroke" />
 -->

            <RelativeLayout
                android:id="@+id/homelayout"
                android:layout_width="fill_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imghome"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/home" />

                <TextView
                    android:id="@+id/txthome"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imghome"
                    android:gravity="center_vertical"
                    android:text="@string/home"
                    android:textColor="@color/darl_gray_text_color"
                    android:textSize="@dimen/font_normal"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/homeline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:background="@color/scanattendee_icons_color"
                    android:visibility="gone" />
            </RelativeLayout>

            <FrameLayout
                android:id="@+id/horinzontal_line_events"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="57dp"
                android:background="@color/header_stroke" />

            <RelativeLayout
                android:id="@+id/ticketlayout"
                android:layout_width="fill_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imgticket"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/tickets" />

                <TextView
                    android:id="@+id/txtticket"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imgticket"
                    android:gravity="center_vertical"
                    android:text="@string/tickets"
                    android:textColor="@color/darl_gray_text_color"
                    android:textSize="@dimen/font_normal"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/ticketline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:background="@color/scanattendee_icons_color"
                    android:visibility="invisible" />
            </RelativeLayout>

            <FrameLayout
                android:id="@+id/horinzontal_line_tickets"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="57dp"
                android:background="@color/header_stroke" />

            <RelativeLayout
                android:id="@+id/listlayout"
                android:layout_width="fill_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imglist"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/attendees" />

                <TextView
                    android:id="@+id/txtlist"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imglist"
                    android:gravity="center_vertical"
                    android:text="@string/list"
                    android:textColor="@color/darl_gray_text_color"
                    android:textSize="@dimen/font_normal"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/listline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:background="@color/scanattendee_icons_color"
                    android:visibility="invisible" />
            </RelativeLayout>

            <FrameLayout
                android:id="@+id/horinzontal_line_attendees"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="57dp"
                android:background="@color/header_stroke" />

            <RelativeLayout
                android:id="@+id/scanlayout"
                android:layout_width="fill_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imgscan"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/scan" />

                <TextView
                    android:id="@+id/txtscan"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imgscan"
                    android:gravity="center_vertical"
                    android:text="@string/scan"
                    android:textColor="@color/darl_gray_text_color"
                    android:textSize="@dimen/font_normal"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/scanline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:background="@color/scanattendee_icons_color"
                    android:visibility="invisible" />
            </RelativeLayout>

            <FrameLayout
                android:id="@+id/horinzontal_line_scan"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="57dp"
                android:background="@color/header_stroke" />
            <RelativeLayout
                android:id="@+id/printattendeeslayout"
                android:layout_width="fill_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imgscan"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/badgeattendee" />

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imgscan"
                    android:gravity="center_vertical"
                    android:text="Bulk Printing"
                    android:textColor="@color/darl_gray_text_color"
                    android:textSize="@dimen/font_normal"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/scanline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:background="@color/scanattendee_icons_color"
                    android:visibility="invisible" />
            </RelativeLayout>

            <FrameLayout
                android:id="@+id/horinzontal_line_scan"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="57dp"
                android:background="@color/header_stroke" />
            <RelativeLayout
                android:id="@+id/offlinescanlayout"
                android:layout_width="fill_parent"
                android:layout_height="55dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imgscan"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/img_offlin_scan" />

                <TextView
                    android:id="@+id/txtofflinescan"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imgscan"
                    android:gravity="center_vertical"
                    android:text="@string/offline_scan"
                    android:textColor="@color/darl_gray_text_color"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/offlinescanline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:background="@color/scanattendee_icons_color"
                    android:visibility="invisible" />
            </RelativeLayout>

            <FrameLayout
                android:id="@+id/horinzontal_line_offlinescan"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="57dp"
                android:background="@color/header_stroke" />

            <RelativeLayout
                android:id="@+id/orderlayout"
                android:layout_width="fill_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imgorder"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/orders" />

                <TextView
                    android:id="@+id/txtorder"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imgorder"
                    android:gravity="center_vertical"
                    android:text="@string/orders"
                    android:textColor="@color/darl_gray_text_color"
                    android:textSize="@dimen/font_normal"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/orderline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:background="@color/scanattendee_icons_color"
                    android:visibility="invisible" />
            </RelativeLayout>

            <FrameLayout
                android:id="@+id/horinzontal_line_orders"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="57dp"
                android:background="@color/header_stroke" />

            <RelativeLayout
                android:id="@+id/settingslayout"
                android:layout_width="fill_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imgsettings"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/img_settings" />

                <TextView
                    android:id="@+id/txtsettings"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imgsettings"
                    android:gravity="center_vertical"
                    android:text="@string/str_settings"
                    android:textColor="@color/darl_gray_text_color"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/settingsline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:background="@color/scanattendee_icons_color"
                    android:visibility="invisible" />
            </RelativeLayout>
            <FrameLayout
                android:id="@+id/horinzontal_line_orders"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="57dp"
                android:background="@color/header_stroke" />

            <RelativeLayout
                android:id="@+id/selfcheckinlayout"
                android:layout_width="fill_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imgselfcheckin"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/menuselfcheckin" />

                <TextView
                    android:id="@+id/txtselfcheckin"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imgselfcheckin"
                    android:gravity="center_vertical"
                    android:text="@string/str_selfchechin"
                    android:textColor="@color/darl_gray_text_color"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/selfchecinline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:layout_alignParentRight="true"
                    android:background="@color/scanattendee_icons_color"
                    android:visibility="invisible" />
            </RelativeLayout>


            <FrameLayout
                android:id="@+id/horinzontal_line_orders"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="57dp"
                android:background="@color/header_stroke" />

            <RelativeLayout
                android:id="@+id/logoutlayout"
                android:layout_width="fill_parent"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/imglogout"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginLeft="15dp"
                    android:src="@drawable/logout" />

                <TextView
                    android:id="@+id/txtlogout"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_toRightOf="@+id/imglogout"
                    android:gravity="center_vertical"
                    android:text="@string/deleteacc"
                    android:textColor="@color/darl_gray_text_color"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:typeface="serif" />

                <View
                    android:id="@+id/logoutline"
                    android:layout_width="5dp"
                    android:layout_height="fill_parent"
                    android:background="@color/scanattendee_icons_color"
                    android:visibility="invisible"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />
            </RelativeLayout>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:layout_marginBottom="15dp"
                android:layout_marginTop="20dp"
                android:drawableTop="@drawable/globalnest_logo"
                android:gravity="bottom|center_horizontal"
                android:text="@string/app_version_production"
                android:textColor="@color/darl_gray_text_color"
                android:textSize="@dimen/font_header" />
        </LinearLayout>
    </ScrollView>

</RelativeLayout>