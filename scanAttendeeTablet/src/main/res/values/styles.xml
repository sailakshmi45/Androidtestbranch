<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.









    -->
    <style name="AppBaseTheme" parent="android:style/Theme.Holo.Light.NoActionBar">
        <item name="android:hint">@color/textcolor</item>
        <item name="android:listSeparatorTextViewStyle">@style/MyOwnListSeperatorTextViewStyle</item>

        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.









        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="Widget.TextView.ListSeparator" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="MyOwnListSeperatorTextViewStyle" parent="Widget.TextView.ListSeparator">
        <item name="android:textColor">#000000</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">#cccccc</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:paddingLeft">12dp</item>
    </style>

    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_up_dialog</item>
        <item name="android:windowExitAnimation">@anim/slide_out_down</item>
    </style>

    <style name="DialogSlideAnim" parent="@android:style/Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/DialogAnimation</item>
    </style>

    <style name="DialogLeftAnimation">
        <item name="android:windowEnterAnimation">@anim/pull_in_from_left</item>
        <item name="android:windowExitAnimation">@anim/pull_out_from_left</item>
    </style>

    <style name="DialogLeftSlideAnim" parent="@android:style/Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/DialogLeftAnimation</item>
    </style>

    <style name="DialogRightAnimation">
        <item name="android:windowEnterAnimation">@anim/pull_out_from_left</item>
        <item name="android:windowExitAnimation">@anim/pull_in_from_left</item>
    </style>

    <style name="DialogRightSlideAnim" parent="@android:style/Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/DialogRightAnimation</item>
    </style>

    <style name="PopupAnimation">
        <item name="android:windowEnterAnimation">@anim/popup_top_slide</item>
        <item name="android:windowExitAnimation">@anim/popup_hide_slide</item>
    </style>

    <style name="PopupSlideAnim" parent="@android:style/Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/PopupAnimation</item>
    </style>

    <style name="MyListFrontContent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/shadow_border</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="CSVTextViewHeaderStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:background">@drawable/colum_cell_shape</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/green_button_color</item>
        <item name="android:textSize">@dimen/font_title</item>
        <item name="android:padding">5dp</item>
    </style>

    //for float text
    <style name="FloatLabel.Label" parent="android:TextAppearance.Small">
        <item name="android:textColor">@color/gray_color</item> //#90A4AE
        <item name="android:textSize">14dp</item>
        <item name="android:typeface">normal</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="MyTextView" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">20dp</item>
    </style>


    <style name="CSVTextViewFieldStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:background">@drawable/colum_cell_shape</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/font_normal</item>
        <item name="android:padding">5dp</item>
    </style>

    <style name="MyEditText">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/edit_text_shadow</item>
        <item name="android:textSize">17dp</item>
        <item name="android:ems">10</item>
        <item name="android:textColor">@color/gray_color</item>
        <item name="android:padding">8dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>

    <style name="MyEditText1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:background">@drawable/line_effect</item>
        <item name="android:textSize">17dp</item>
        <item name="android:ems">10</item>
        <item name="android:textColor">@color/gray_color</item>
        <item name="android:padding">8dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:layout_marginLeft">15dp</item>
    </style>

    <style name="DialogBottomAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_bottom_dialog</item>
        <item name="android:windowExitAnimation">@anim/slide_bottm_down</item>
    </style>

    <style name="DialogBottomSlideAnim" parent="@android:style/Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/DialogBottomAnimation</item>
    </style>

    <style name="StyleEdit" parent="@android:style/Widget.EditText">
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="CustomForDialog" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowFrame">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="SwitchTextAppearance" parent="@android:style/Widget.Holo.Button"></style>
    <!-- tab view style -->
    <style name="text_title">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>


    <declare-styleable name="CircularProgressView">
        <attr name="progressColor" format="color" />
        <attr name="progressText" format="string" />
        <attr name="progressTextColor" format="color" />
        <attr name="progressTextFont" format="string" />
        <attr name="progressTextSize" format="integer" />
        <attr name="borderOffset" format="integer" />
        <attr name="fadeTime" format="integer" />
    </declare-styleable>

    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <style name="MyCustomTheme" parent="@android:style/Theme.Dialog">
       <item name="android:windowFrame">@null</item>
       <item name="android:windowIsFloating">true</item>
      <!-- <item name="android:windowIsTranslucent">true</item>-->
       <item name="android:windowNoTitle">true</item>
       <item name="android:windowAnimationStyle">@style/MyAnimation.Window</item>
   </style>

   <style name="MyAnimation.Window" parent="@android:style/Animation.Activity">
       <item name="android:windowEnterAnimation">@anim/zoom_in</item>
       <item name="android:windowExitAnimation">@anim/zoom_out</item>
   </style>
   
   <style name="MyTresperant" parent="@android:style/Theme.Holo.Light.Dialog.NoActionBar">
       <item name="android:windowFrame">@null</item>
       <item name="android:windowIsFloating">true</item>
      <!-- <item name="android:windowIsTranslucent">true</item>-->
       <item name="android:windowNoTitle">true</item>
       <item name="android:windowBackground">@color/transparent</item>
       <item name="android:windowContentOverlay">@null</item>
   </style>
